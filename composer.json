{
    "name": "amansrivastava/widen-cli",
    "description": "CLI app for Widen API calls.",
    "keywords": ["widen", "acquia", "laravel zero", "console", "cli"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Aman Srivastava",
            "email": "aman.srivastava@live.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/http": "^8",
        "spatie/simple-excel": "^1.13"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1",
        "laravel-zero/framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/widen-cli"]
}
